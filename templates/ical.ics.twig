BEGIN:VCALENDAR
VERSION:2.0
CALSCALE:GREGORIAN
PRODID:-//rota.youd.family//RotaProcessor//EN
X-WR-CALNAME:{{ placement.name }}
REFRESH-INTERVAL;VALUE=DURATION:PT4H
X-PUBLISHED-TTL:PT4H
{% for shift in shifts %}
BEGIN:VEVENT
UID:{{ placement.id }}-{{ loop.index }}
DTSTART:{{ shift.start | date('Ymd') }}T{{ shift.start | date('His') }}
DTEND:{{ shift.end | date('Ymd') }}T{{ shift.end | date('His') }}
CATEGORIES:{{ placement.calendarCategory }}
ICAL-DATA:{}
SUMMARY:{{ placement.prefix }}{{ shift.type }}
END:VEVENT
{% endfor %}
END:VCALENDAR
